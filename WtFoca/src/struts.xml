<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
   "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
   "http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>
   <package name="default" extends="struts-default">
		<action name="openLogin" class="com.core.web.actions.login.LoginAction" method="open">
			<result name="success" type="dispatcher">/focaWeb/login/login.jsp</result>
			<result name="error" type="dispatcher">/focaWeb/login/login.jsp</result>
		</action>
   </package>
 
<constant name="struts.action.excludePattern" value="/xmlServletpath/?,/registropath/?,/postagempath/?,/upLoadPostagempath/?,/imgServletpath/?,/generate/?,/like/?"/>
</struts>